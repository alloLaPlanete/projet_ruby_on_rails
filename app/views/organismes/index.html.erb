<p id="notice"><%= notice %></p>

<%= current_deviseuser.id%>

<h1>Listing Organismes</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Adresse</th>
      <th>Telephone</th>
      <th>Courriel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organismes.each do |organisme| %>
    <%if current_deviseuser.id == organisme.deviseuser_id%>
      <tr>
        <td><%= organisme.nom %></td>
        <td><%= organisme.adresse %></td>
        <td><%= organisme.telephone %></td>
        <td><%= organisme.courriel %></td>
        <td><%= link_to 'Show', organisme %></td>
        <td><%= link_to 'Edit', edit_organisme_path(organisme) %></td>
        <td><%= link_to 'Destroy', organisme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organisme', new_organisme_path %>
