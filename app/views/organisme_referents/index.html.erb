<p id="notice"><%= notice %></p>

<h1>Listing Organisme Referents</h1>

<hr />
<table class="table table-hover">
  <thead>
    <tr>
      <th>Nomorganisme</th>
      <th>Adressenocivique</th>
      <th>Adresserue</th>
      <th>Adresseville</th>
      <th>Adresseprovince</th>
      <th>Adresseetat</th>
      <th>Adressecodepostal</th>
      <th>Telephonebureau</th>
      <th>Adressetelecopie</th>
      <th>Courriel</th>
      <th>Siteweb</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organisme_referents.each do |organisme_referent| %>
      <tr>
        <td><%= organisme_referent.nomOrganisme %></td>
        <td><%= organisme_referent.adresseNoCivique %></td>
        <td><%= organisme_referent.adresseRue %></td>
        <td><%= organisme_referent.adresseVille %></td>
        <td><%= organisme_referent.adresseProvince %></td>
        <td><%= organisme_referent.adresseEtat %></td>
        <td><%= organisme_referent.adresseCodePostal %></td>
        <td><%= organisme_referent.telephoneBureau %></td>
        <td><%= organisme_referent.adresseTelecopie %></td>
        <td><%= organisme_referent.courriel %></td>
        <td><%= organisme_referent.siteWeb %></td>
        <td><%= link_to 'Show', organisme_referent %></td>
        <td><%= link_to 'Edit', edit_organisme_referent_path(organisme_referent) %></td>
        <td><%= link_to 'Destroy', organisme_referent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Organisme referent', new_organisme_referent_path %>
